<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jf.desarrollos.arroyitocomercial" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission android:protectionLevel="signature"
        android:name="jf.desarrollos.arroyitocomercial.permission.C2D_MESSAGE" />
    <uses-permission android:name="jf.desarrollos.arroyitocomercial.permission.C2D_MESSAGE" />

    <application

        android:allowBackup="true"
        android:icon="@drawable/icono_inicio"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="jf.desarrollos.arroyitocomercial.Controlador.Volley.AppController">

        <activity
            android:name="jf.desarrollos.arroyitocomercial.Vista.Actividad.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="jf.desarrollos.arroyitocomercial.Vista.Actividad.MainActivity"
            android:screenOrientation="portrait"
            android:icon="@mipmap/ic_menu"/>

        <activity android:name="jf.desarrollos.arroyitocomercial.Vista.Actividad.ComercioActivity"
            android:screenOrientation="portrait"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCd5lcZJwIr4uAFq4ae19Fyu2UG1jFTXAM" />

        <service android:name=".Controlador.Firebase.IFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name=".Controlador.Firebase.IFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>
